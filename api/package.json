{
  "name": "curitransporte-api",
  "version": "1.0.0",
  "description": "API Curitransporte",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "start:dev": "nodemon src/index.js",
    "start:https": "cross-env ENABLE_HTTPS=true HTTPS_PORT=3443 nodemon src/index.js",
    "start:https:strict": "cross-env ENABLE_HTTPS=true HTTPS_PORT=3443 TLS_STRICT=true nodemon src/index.js",
    "dev": "nodemon src/index.js",
    "generate-certs": "node generate-certs.js",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "keywords": [
    "api",
    "express",
    "curitransporte"
  ],
  "author": "Tu Equipo",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.0",
    "express-rate-limit": "^6.7.0",
    "geolib": "^3.3.4",
    "helmet": "^7.0.0",
    "joi": "^17.9.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-cron": "^4.2.1",
    "pg": "^8.16.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.0.0"
  }
}
